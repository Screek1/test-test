<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
        xmlns:image="http://www.google.com/schemas/sitemap-image/1.1">
    {% for item in urls %}
    <url>
        <loc>{{ item.url }}</loc>

        {% for image in item.images %}
        <image:image>
            <image:loc>{{ image }}</image:loc>
        </image:image>
        {% endfor %}
    </url>
    {% endfor %}
</urlset>